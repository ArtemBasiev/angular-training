      <endpoint address="http://localhost:53862/AsmxCategoryService.asmx" binding="basicHttpBinding" bindingConfiguration="AsmxCategoryServiceSoap" contract="AsmxCategoryServiceReference.AsmxCategoryServiceSoap" name="AsmxCategoryServiceSoap" />
      <endpoint address="http://localhost:53862/AsmxBlogUserService.asmx" binding="basicHttpBinding" bindingConfiguration="AsmxBlogUserServiceSoap" contract="AsmxBlogUserServiceReference.AsmxBlogUserServiceSoap" name="AsmxBlogUserServiceSoap" />
      <endpoint address="http://localhost:53862/AsmxCommentService.asmx" binding="basicHttpBinding" bindingConfiguration="AsmxCommentServiceSoap" contract="AsmxCommentServiceReference.AsmxCommentServiceSoap" name="AsmxCommentServiceSoap" />
      <endpoint address="http://localhost:53862/AsmxPostService.asmx" binding="basicHttpBinding" bindingConfiguration="AsmxPostServiceSoap" contract="AsmxPostServiceReference.AsmxPostServiceSoap" name="AsmxPostServiceSoap" />
    </client>
